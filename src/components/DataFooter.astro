---
const dataList = [
    {
        title: 'Nosotros',
        links: [
            { title: 'Medicina Reproductiva', href: '/'},
            { title: 'Servicios', href: '/' },
            { title: 'Profesionales', href: '/' },
            { title: 'Prensa', href: '/' },
            { title: 'Contacto', href: '/' }
        ]
    },
    {
        title: 'Contacto',
        links: [
            { title: '20 de Febrero 1394', href: '/' },
            { title: 'Salta Argentina', href: '/' },
            { title: '+54 9 3874211437', href: '/' },
            { title: '+54 9 3875898999', href: '/' }
        ]
    }
]
---
{
    dataList.map((data) => {
        const { title, links } = data; 
        return (
            <div>
                <h2 class="mb-2 text-sm font-semibold text-gray-700 uppercase dark:text-white">{title}</h2>
                <ul class="text-gray-500 text-sm">
                    {
                        links.map((link) => (
                            <li>
                                <a href={link.href} class="hover:underline">
                                    {link.title}
                                </a>
                            </li>
                        ))
                    }
                </ul>
            </div>
        )
    })
}

